{
  "compileOnSave": false,
  "compilerOptions": {
    "jsx": "preserve",
    "allowJs": true,
    "checkJs": true,
    // "strict": true,
    "noEmit": true,
    "moduleResolution": "node",
    "target": "ESNext",
    "module": "esnext",
    "resolveJsonModule": true,
    "allowSyntheticDefaultImports": true,
    "downlevelIteration": true,
    "lib": ["dom"]
  },
  "typeAcquisition": {
    "enable": true
  }
}
